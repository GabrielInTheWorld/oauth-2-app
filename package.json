{
  "name": "openslides-auth-service",
  "version": "1.0.0",
  "description": "Service for OpenSlides which ...",
  "main": "main.js",
  "directories": {
    "test": "jest"
  },
  "scripts": {
    "postinstall": "npm run uptodate",
    "cleanup": "npm run prettify-write && npm run linter-write",
    "start": "node build/index.js",
    "start:electron": "npm run build-electron && electron .",
    "start:electron-only": "electron .",
    "dev": "nodemon src/index.ts",
    "linter-write": "tslint --fix -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "prettify-write": "prettier --config ./.prettierrc --write \"src/**/*{.ts,.js,.json,.css,.scss}\"",
    "lint-check": "tslint -c ./tslint.json 'src/**/*{.ts,.tsx}'",
    "prettify-check": "prettier --config ./.prettierrc --list-different \"src/{app,environments}/**/*{.ts,.js,.json,.css,.scss}\"",
    "build-all": "npm run build-ts && npm run build-client",
    "build-electron": "npm run build-ts && cd client && npm run build:electron",
    "build-ts": "tsc",
    "build-client": "cd client && npm install && npm install --only=dev && npm run build",
    "heroku-postbuild": "npm run build-all",
    "prebuild": "echo false > .buildstatus",
    "postbuild": "echo true > .buildstatus",
    "test": "jest --passWithNoTests",
    "uptodate": "npm update && npm update --dev",
    "make-badges": "node_modules/.bin/jest-badges-readme",
    "make-readme": "npm t && npm run make-badges"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenSlides/openslides-auth-service.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenSlides/openslides-auth-service/issues"
  },
  "homepage": "https://github.com/OpenSlides/openslides-auth-service#readme",
  "devDependencies": {
    "@olavoparno/jest-badges-readme": "^1.4.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/jest": "^26.0.19",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.13.39",
    "@types/pouchdb": "^6.4.0",
    "@types/qs": "^6.9.2",
    "@types/randomstring": "^1.1.6",
    "@types/sendmail": "^1.4.2",
    "@types/sha.js": "^2.4.0",
    "@types/superagent": "^4.1.10",
    "@types/websocket": "^1.0.1",
    "electron-rebuild": "^2.3.5",
    "jest": "^26.6.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "superagent": "^6.1.0",
    "sync-request": "^6.1.0",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.1"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "base-coding": "^1.0.3",
    "base32-encode": "^1.1.1",
    "bignumber.js": "^9.0.1",
    "body-parser": "^1.19.0",
    "cbor": "^7.0.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.2.0",
    "dotenv": "^8.2.0",
    "electron": "^11.3.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-react-views": "^0.11.0",
    "final-otp": "^1.0.3",
    "jsonwebtoken": "^8.5.1",
    "pouchdb": "^7.2.1",
    "pouchdb-adapter-memory": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "qs": "^6.9.4",
    "randomstring": "^1.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "reactive-websocket": "^1.0.4",
    "reflect-metadata": "^0.1.13",
    "sendmail": "^1.6.1",
    "sha.js": "^2.4.11",
    "ts-node": "^8.10.1",
    "tslib": "^1.11.2",
    "typescript": "^3.8.3",
    "uuidv4": "^6.0.6",
    "websocket": "^1.0.33",
    "xterm": "^4.10.0"
  }
}
